{
  "$defs": {
    "Condition": {
      "properties": {
        "job_id": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            }
          ],
          "title": "Job Id"
        },
        "project_id": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            }
          ],
          "title": "Project Id"
        },
        "statuses": {
          "default": [
            10,
            20,
            30
          ],
          "description": "The statuses that will trigger the job. 10=success 20=error 30=cancelled",
          "items": {
            "enum": [
              10,
              20,
              30
            ],
            "type": "integer"
          },
          "title": "Statuses",
          "type": "array"
        }
      },
      "required": [
        "job_id",
        "project_id"
      ],
      "title": "Condition",
      "type": "object"
    },
    "Date": {
      "properties": {
        "type": {
          "title": "Type",
          "type": "string"
        },
        "cron": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Cron"
        }
      },
      "required": [
        "type"
      ],
      "title": "Date",
      "type": "object"
    },
    "Execution": {
      "properties": {
        "timeout_seconds": {
          "default": 0,
          "title": "Timeout Seconds",
          "type": "integer"
        }
      },
      "title": "Execution",
      "type": "object"
    },
    "JobCompletionTriggerCondition": {
      "properties": {
        "condition": {
          "$ref": "#/$defs/Condition"
        }
      },
      "required": [
        "condition"
      ],
      "title": "JobCompletionTriggerCondition",
      "type": "object"
    },
    "JobDefinition": {
      "description": "A definition for a dbt Cloud job.",
      "properties": {
        "id": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Id"
        },
        "identifier": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Identifier"
        },
        "account_id": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            }
          ],
          "title": "Account Id"
        },
        "project_id": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            }
          ],
          "title": "Project Id"
        },
        "environment_id": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            }
          ],
          "title": "Environment Id"
        },
        "dbt_version": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Dbt Version"
        },
        "name": {
          "title": "Name",
          "type": "string"
        },
        "settings": {
          "$ref": "#/$defs/Settings"
        },
        "execution": {
          "allOf": [
            {
              "$ref": "#/$defs/Execution"
            }
          ],
          "default": {
            "timeout_seconds": 0
          }
        },
        "deferring_job_definition_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Deferring Job Definition Id"
        },
        "deferring_environment_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Deferring Environment Id"
        },
        "run_generate_sources": {
          "title": "Run Generate Sources",
          "type": "boolean"
        },
        "execute_steps": {
          "items": {
            "type": "string"
          },
          "title": "Execute Steps",
          "type": "array"
        },
        "generate_docs": {
          "title": "Generate Docs",
          "type": "boolean"
        },
        "schedule": {
          "$ref": "#/$defs/Schedule"
        },
        "triggers": {
          "$ref": "#/$defs/Triggers"
        },
        "description": {
          "default": "",
          "title": "Description",
          "type": "string"
        },
        "state": {
          "default": 1,
          "title": "State",
          "type": "integer"
        },
        "job_type": {
          "default": "scheduled",
          "enum": [
            "scheduled",
            "merge",
            "ci",
            "other"
          ],
          "title": "Job Type",
          "type": "string"
        },
        "triggers_on_draft_pr": {
          "default": false,
          "title": "Triggers On Draft Pr",
          "type": "boolean"
        },
        "job_completion_trigger_condition": {
          "anyOf": [
            {
              "$ref": "#/$defs/JobCompletionTriggerCondition"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        },
        "custom_environment_variables": {
          "default": [],
          "description": "Dictionary of custom environment variables name and value for the job. The env var name must start with DBT_.",
          "items": {
            "additionalProperties": false,
            "patternProperties": {
              "^DBT_": {
                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "number"
                  },
                  {
                    "type": "boolean"
                  }
                ]
              }
            },
            "type": "object"
          },
          "title": "Custom Environment Variables",
          "type": "array"
        }
      },
      "required": [
        "account_id",
        "project_id",
        "environment_id",
        "name",
        "settings",
        "run_generate_sources",
        "execute_steps",
        "generate_docs",
        "schedule",
        "triggers"
      ],
      "title": "JobDefinition",
      "type": "object"
    },
    "Schedule": {
      "properties": {
        "cron": {
          "title": "Cron",
          "type": "string"
        },
        "date": {
          "anyOf": [
            {
              "$ref": "#/$defs/Date"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        },
        "time": {
          "anyOf": [
            {
              "$ref": "#/$defs/Time"
            },
            {
              "type": "null"
            }
          ],
          "default": null
        }
      },
      "required": [
        "cron"
      ],
      "title": "Schedule",
      "type": "object"
    },
    "Settings": {
      "properties": {
        "threads": {
          "default": 4,
          "title": "Threads",
          "type": "integer"
        },
        "target_name": {
          "default": "default",
          "title": "Target Name",
          "type": "string"
        }
      },
      "title": "Settings",
      "type": "object"
    },
    "Time": {
      "properties": {
        "type": {
          "title": "Type",
          "type": "string"
        },
        "interval": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Interval"
        }
      },
      "required": [
        "type"
      ],
      "title": "Time",
      "type": "object"
    },
    "Triggers": {
      "properties": {
        "github_webhook": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "boolean"
            }
          ],
          "default": false,
          "title": "Github Webhook"
        },
        "git_provider_webhook": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "boolean"
            }
          ],
          "default": false,
          "title": "Git Provider Webhook"
        },
        "custom_branch_only": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "boolean"
            }
          ],
          "default": false,
          "title": "Custom Branch Only"
        },
        "schedule": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "boolean"
            }
          ],
          "default": false,
          "title": "Schedule"
        },
        "on_merge": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "boolean"
            }
          ],
          "default": false,
          "title": "On Merge"
        }
      },
      "title": "Triggers",
      "type": "object"
    }
  },
  "description": "Internal representation of a Jobs as Code configuration file.",
  "properties": {
    "jobs": {
      "additionalProperties": {
        "$ref": "#/$defs/JobDefinition"
      },
      "title": "Jobs",
      "type": "object"
    }
  },
  "required": [
    "jobs"
  ],
  "title": "Config",
  "type": "object"
}